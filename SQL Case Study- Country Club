   "cell_type": "code",
   "execution_count": 6,
   "id": "b9135a5e",
   "metadata": {},
   "outputs": [],
   "source": [
    "charge_per_visit=[]\n",
    "\n",
    "for visit in df['memid']:\n",
    "    if df['memid'][visit]!= 0:\n",
    "        per_visit= df['membercost'][visit]\n",
    "    else:\n",
    "        df['guestcost'][visit]\n",
    "        per_visit= df['guestcost'][visit]\n",
    "    charge_per_visit.append(per_visit)"
   ]
  },
