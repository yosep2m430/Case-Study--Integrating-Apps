   "outputs": [],
   "source": [
    "engine= create_engine('sqlite:///sqlite_db_pythonsqlite.db')\n",
    "con=engine.connect()\n",
    "rs=con.execute(\"SELECT DISTINCT* FROM Bookings INNER JOIN Facilities ON Bookings.facid= Facilities.facid\" )\n",
    "df= pd.DataFrame(rs.fetchall())\n",
    "df.columns= rs.keys()\n",
    "con.close()\n",
    "\n",
    "#Q12"
   ]
